{
  "name": "vite-react-temp",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "start": "vite --mode dev",
    "build": "tsc && vite build --mode pro",
    "preview": "vite preview",
    "analyzer": "ANALYZER=true vite build --mode",
    "commit": "git-cz",
    "format": "prettier --write .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:fix": "npm run lint:script && npm run lint:style && prettier --write .",
    "lint:script": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:style": "stylelint 'src/**/*.{css,scss,less}' --fix",
    "lint:cspell": "cspell **",
    "lang:extract": "formatjs extract \"src/**/!(*.d).{js,jsx,ts,tsx}\" --out-file src/locale/extract/zh-CN.json",
    "lang:compile": "formatjs compile-folder src/locale/extract src/locale/compile",
    "lang": "npm run lang:extract && npm run lang:compile",
    "exportNginx": "tsx scripts/export-nginx.ts",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:script"
    ],
    "*.{css,scss,less}": [
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.17.2",
    "@ant-design/icons": "^5.2.6",
    "@codemirror/commands": "^6.3.0",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/state": "^6.3.1",
    "@codemirror/view": "^6.22.0",
    "@redux-devtools/extension": "^3.2.6",
    "ahooks": "^3.7.8",
    "antd": "^5.11.1",
    "axios": "^1.6.1",
    "classnames": "^2.3.2",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "file-saver": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-intl": "^6.5.2",
    "react-redux": "^8.1.3",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "reselect": "^4.1.8",
    "ts-md5": "^1.3.1",
    "typesafe-actions": "^5.1.0",
    "url-join": "^5.0.0",
    "uuid": "^9.0.1",
    "video.js": "^8.6.1",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.23.3",
    "@babel/plugin-proposal-do-expressions": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.3",
    "@babel/plugin-transform-numeric-separator": "^7.23.3",
    "@babel/plugin-transform-optional-chaining": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/plugin-transform-private-property-in-object": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@formatjs/cli": "^6.2.2",
    "@formatjs/ts-transformer": "^3.13.7",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/url-join": "^4.0.3",
    "@types/uuid": "^9.0.7",
    "@types/video.js": "^7.3.56",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "autoprefixer": "^10.4.16",
    "babel-plugin-formatjs": "^10.5.8",
    "commitizen": "^4.3.0",
    "cspell": "^8.0.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.24.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-formatjs": "^4.11.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "tsx": "^4.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-static-copy": "^0.17.0"
  }
}
